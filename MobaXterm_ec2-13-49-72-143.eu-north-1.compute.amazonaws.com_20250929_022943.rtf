\par \cf0\highlight0 import os\cf1\highlight2 
\par \cf0\highlight0 import tarfile\cf1\highlight2 
\par \cf0\highlight0 import shutil\cf1\highlight2 
\par \cf0\highlight0 import gradio as gr\cf1\highlight2 
\par \cf0\highlight0 from langchain.text_splitter import RecursiveCharacterTextSplitter\cf1\highlight2 
\par \cf0\highlight0 from langchain_community.vectorstores import Chroma\cf1\highlight2 
\par \cf0\highlight0 from langchain_community.embeddings import HuggingFaceEmbeddings\cf1\highlight2 
\par \cf0\highlight0 from langchain_ollama import OllamaLLM\cf1\highlight2 
\par \cf0\highlight0 from langchain.chains import RetrievalQA\cf1\highlight2 
\par 
\par \cf0\highlight0 UPLOAD_DIR = "uploaded_reports"\cf1\highlight2 
\par \cf0\highlight0 EXTRACT_DIR = "extracted_reports"\cf1\highlight2 
\par \cf0\highlight0 DB_DIR = "vector_db"\cf1\highlight2 
\par \cf0\highlight0 os.makedirs(UPLOAD_DIR, exist_ok=True)\cf1\highlight2 
\par \cf0\highlight0 os.makedirs(EXTRACT_DIR, exist_ok=True)\cf1\highlight2 
\par 
\par \cf0\highlight0 qa = None  # Global QA chain\cf1\highlight2 
\par 
\par \cf0\highlight0 def process_sosreport(sos_file):\cf1\highlight2 
\par \cf0\highlight0     global qa\cf1\highlight2 
\par \cf0\highlight0     status_msgs = []\cf1\highlight2 
\par 
\par \cf0\highlight0     # Save uploaded file\cf1\highlight2 
\par \cf0\highlight0     upload_path = os.path.join(UPLOAD_DIR, sos_file.name)\cf1\highlight2 
\par \cf0\highlight0     with open(upload_path, "wb") as f:\cf1\highlight2 
\par \cf0\highlight0         f.write(sos_file.read())\cf1\highlight2 
\par 
\par \cf0\highlight0     # Cleanup old extraction & DB\cf1\highlight2 
\par \cf0\highlight0     if os.path.exists(EXTRACT_DIR):\cf1\highlight2 
\par \cf0\highlight0         shutil.rmtree(EXTRACT_DIR)\cf1\highlight2 
\par \cf0\highlight0     if os.path.exists(DB_DIR):\cf1\highlight2 
\par \cf0\highlight0         shutil.rmtree(DB_DIR)\cf1\highlight2 
\par \cf0\highlight0     os.makedirs(EXTRACT_DIR, exist_ok=True)\cf1\highlight2 
\par 
\par \cf0\highlight0     # Extract tarball\cf1\highlight2 
\par \cf0\highlight0     extract_path = os.path.join(EXTRACT_DIR, sos_file.name.replace(".tar.xz","").replace(".tar.gz",""))\cf1\highlight2 
\par \cf0\highlight0     os.makedirs(extract_path, exist_ok=True)\cf1\highlight2 
\par \cf0\highlight0     with tarfile.open(upload_path, "r:*") as tar:\cf1\highlight2 
\par \cf0\highlight0         tar.extractall(extract_path)\cf1\highlight2 
\par \cf0\highlight0     status_msgs.append(f"\u9989? Extracted SOS report to \{extract_path\}")\cf1\highlight2 
\par 
\par \cf0\highlight0     # Collect readable files\cf1\highlight2 
\par \cf0\highlight0     docs = []\cf1\highlight2 
\par \cf0\highlight0     for root, dirs, files in os.walk(extract_path):\cf1\highlight2 
\par \cf0\highlight0         for fname in files:\cf1\highlight2 
\par \cf0\highlight0             fpath = os.path.join(root, fname)\cf1\highlight2 
\par \cf0\highlight0             if fname.endswith((".log", ".txt", ".conf", ".out", ".err", "dmesg")):\cf1\highlight2 
\par \cf0\highlight0                 try:\cf1\highlight2 
\par \cf0\highlight0                     with open(fpath, "r", errors="ignore") as f:\cf1\highlight2 
\par \cf0\highlight0                         docs.append(f.read())\cf1\highlight2 
\par \cf0\highlight0                 except:\cf1\highlight2 
\par \cf0\highlight0                     status_msgs.append(f"\u9888?\cf1\highlight2\u65039?\cf0\highlight0  Could not read \{fpath\}")\cf1\highlight2 
\par 
\par \cf0\highlight0     if not docs:\cf1\highlight2 
\par \cf0\highlight0         return "\u9888?\cf1\highlight2\u65039?\cf0\highlight0  No readable files found in sosreport."\cf1\highlight2 
\par 
\par \cf0\highlight0     # Split into chunks\cf1\highlight2 
\par \cf0\highlight0     splitter = RecursiveCharacterTextSplitter(chunk_size=1000, chunk_overlap=200)\cf1\highlight2 
\par \cf0\highlight0     chunks = splitter.create_documents(docs)\cf1\highlight2 
\par 
\par \cf0\highlight0     # Create embeddings & vector DB\cf1\highlight2 
\par \cf0\highlight0     embeddings = HuggingFaceEmbeddings(model_name="sentence-transformers/all-MiniLM-L6-v2")\cf1\highlight2 
\par \cf0\highlight0     vectordb = Chroma.from_documents(chunks, embeddings, persist_directory=DB_DIR)\cf1\highlight2 
\par \cf0\highlight0     vectordb.persist()\cf1\highlight2 
\par \cf0\highlight0     status_msgs.append("\u9989? Vector DB created")\cf1\highlight2 
\par 
\par \cf0\highlight0     # Setup QA chain with Ollama\cf1\highlight2 
\par \cf0\highlight0     llm = OllamaLLM(model="llama3")\cf1\highlight2 
\par \cf0\highlight0     retriever = vectordb.as_retriever(search_kwargs=\{"k":5\})\cf1\highlight2 
\par \cf0\highlight0     qa = RetrievalQA.from_chain_type(llm=llm, retriever=retriever)\cf1\highlight2 
